  },
  "homepage": "https://github.com/jupyterlab/jupyterlab_app#readme",
  "devDependencies": {
    "css-loader": "^0.27.3",
    "file-loader": "^0.10.1",
    "fs-extra": "^2.1.2",
    "handlebars": "^4.0.6",
    "json-loader": "^0.5.4",
    "style-loader": "^0.13.1",
    "typescript": "^2.2.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1"
  },
  "dependencies": {
    "@types/handlebars": "^4.0.33",
    "electron": "^1.6.11",
